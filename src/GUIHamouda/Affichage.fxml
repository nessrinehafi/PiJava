<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.AffichageController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" layoutY="169.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/hamouda.jpg" />
         </image>
      </ImageView>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/pic2.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="table" layoutX="14.0" layoutY="143.0" prefHeight="141.0" prefWidth="481.0">
        <columns>
          <TableColumn fx:id="categorie" prefWidth="82.0" text="Categorie" />
          <TableColumn fx:id="animal" prefWidth="68.0" text="Animal" />
            <TableColumn fx:id="race" prefWidth="73.0" text="Race" />
            <TableColumn fx:id="description" prefWidth="117.0" text="Description" />
            <TableColumn fx:id="date" prefWidth="65.0" text="Date" />
            <TableColumn fx:id="prix" prefWidth="101.0" text="Prix" />
        </columns>
      </TableView>
      <Button fx:id="recherche" layoutX="265.0" layoutY="90.0" mnemonicParsing="false" onAction="#Recherche" prefHeight="25.0" prefWidth="89.0" text="Recherche " />
      <Button fx:id="supprimer" layoutX="300.0" layoutY="351.0" mnemonicParsing="false" onAction="#Supprimer" text="Supprimer" />
      <Button fx:id="modifier" layoutX="204.0" layoutY="332.0" mnemonicParsing="false" onAction="#Modifier" text="Modifier" />
      <ComboBox fx:id="cb" layoutX="67.0" layoutY="90.0" prefHeight="25.0" prefWidth="163.0" promptText="Choisir une categorie" />
      <Button fx:id="retour" layoutX="6.0" layoutY="6.0" mnemonicParsing="false" onAction="#Retour" prefHeight="25.0" prefWidth="61.0" text="Retour" />
      <Button fx:id="vente" layoutX="46.0" layoutY="326.0" mnemonicParsing="false" onAction="#Add" text="Ajouter une annonce" />
      <Button fx:id="demande" layoutX="455.0" layoutY="351.0" mnemonicParsing="false" onAction="#demande" text="Voir mes demandes" />
      <ImageView fx:id="image" fitHeight="115.0" fitWidth="152.0" layoutX="440.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
